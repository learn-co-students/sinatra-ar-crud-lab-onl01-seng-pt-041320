<form action="/articles/<%= @article.id %>" method="post">
<input id="hidden" type="hidden" name="_method" value="PATCH">
Title: <input type="text" id="title" name="title" value = "<%= @article.title %>">
Content: <input type="text" id="content" name="content" value = "<%= @article.content %>">
<input type="submit" value="submit">
</form>

